    <style name="ActionBar.Solid.Action_bar_indigo_light" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_action_bar_indigo_light</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_action_bar_indigo_light</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_action_bar_indigo_light</item>
        <item name="progressBarStyle">@style/ProgressBar.Action_bar_indigo_light</item>
        <item name="titleTextStyle">@style/myTheme.ActionBar.TitleText</item>
    </style>

