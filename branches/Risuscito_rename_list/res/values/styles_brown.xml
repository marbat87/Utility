    <style name="ActionBar.Solid.Action_bar_brown_light" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_action_bar_brown_light</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_action_bar_brown_light</item>
